<opml version="2.0">
    <body>
        <outline text="Feedsubs Subscriptions" title="Feedsubs Subscriptions">

            {% for subscription in object_list %}
                <outline text="{{ subscription.feed.name }}"
                         title="{{ subscription.feed.name }}"
                         type="rss"
                         xmlUrl="{{ subscription.feed.uri }}"/>
            {% endfor %}

        </outline>
    </body>
</opml>